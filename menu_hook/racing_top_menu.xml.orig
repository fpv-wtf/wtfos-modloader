                    widget.icon.selimage.name="sd/active.png"
                    widget.title.text="msg_playback_left_menu_btn"
                    widget.menubutton.data = "local_media"
                    />
                <menuitem childwindow="racing_settings_win"
                    widget.icon.image.name="setting/default.png"
		    widget.icon.selimage.name="setting/active.png"
                    widget.title.text="msg_setting_left_menu_btn"
                    widget.menubutton.data = "Settings"
                    />
                <menuitem childwindow="racing_sdr_debug_win"
                    widget.icon.image.name=""
                    widget.icon.selimage.name=""
                    widget.title.text="Debug SDR"
                    widget.menubutton.data = "SDR"
                    />
                <menuitem childwindow="racing_debug_win"
                    widget.icon.image.name=""
                    widget.icon.selimage.name=""
                    widget.title.text="Debug OSD"
                    widget.menubutton.data = "Debug"
                    />
