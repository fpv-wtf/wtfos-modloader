    <childwindow name="racing_top_menu_win" dx="0px" dy="24px" w="276px" h="768px" alignment="top-left" show="false" >
            <menu name="racing_top_menu" class="tool_setting_label_menu_class" inputmode="MOVE" vloop="true" alignment="center">
                <menuitem childwindow="racing_chnl_win"
		    widget.icon.image.name="player/default.png"
		    widget.icon.selimage.name="player/active.png"
                    widget.title.text="msg_player_left_menu_btn"
                    widget.menubutton.data = "Pilot Freq Sel"
                    />
